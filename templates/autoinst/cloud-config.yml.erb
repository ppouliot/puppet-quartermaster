#cloud-config
coreos:
    etcd:
        name: coreos-1
    # generate a new token for each unique cluster from https://discovery.etcd.io/new
#        discovery: [your discovery token]
        discovery: "https://discovery.etcd.io/abe50e15454eef382f4db9947ad526f8"
        addr: $private_ipv4:4001
        peer-addr: $private_ipv4:7001
    fleet:
        public-ip: $private_ipv4
    # uncomment for azure.
    #    metadata: region=us-east
    units:
      - name: etcd.service
        command: start
      - name: fleet.service
        command: start
        after: etcd.service
      - name: sshd.socket
        command: restart
        runtime: true
        content: |
          [Socket]
          ListenStream=2222
          Accept=yes
ssh_authorized_keys:
  - ssh-rsa  peter@hackintosh.local
  - ssh-ed25519 AAAAC3NzaC1lZDI1NTE5AAAAIKetw3EOkjAc3+QuDbumo1GovGzCZbQ7McDDFPyis779 peter@quartermaster
  - ssh-rsa  hyper-v_ci@microsoft.com

hostname: coreos-1
