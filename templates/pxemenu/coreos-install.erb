LABEL <%= @name %>
        MENU LABEL (<%= @release %>-<%= @p_arch %>): CoreOS ^Manual Installation 
        KERNEL ../<%= @distro %>/<%= @p_arch %>/<%= @target_kernel %>
        APPEND initrd=../<%= @distro %>/<%= @p_arch %>/<%= @target_initrd %> rootfstype=btrfs coreos.autologin

LABEL <%= @name %>-<%= @autofile %>-install
        MENU LABEL (<%= @release %>-<%= @p_arch %>): CoreOS ^<%= @autofile.capitalize %> Installation 
        KERNEL ../<%= @distro %>/<%= @p_arch %>/<%= @target_kernel %>
        APPEND initrd=../<%= @distro %>/<%= @p_arch %>/<%= @target_initrd %> cloud-config-url=http://<%= @fqdn %>/<%= @distro %>/<%= @autofile %>/<%= @distro %>.pxe_installer.sh

LABEL <%= @name %>-<%= @autofile %>-running-instance
        MENU LABEL (<%= @release %>-<%= @p_arch %>): CoreOS <%= @autofile.capitalize %> ^Running Instance
        KERNEL ../<%= @distro %>/<%= @p_arch %>/<%= @target_kernel %>
        APPEND initrd=../<%= @distro %>/<%= @p_arch %>/<%= @target_initrd %> cloud-config-url=http://<%= @fqdn %>/<%= @distro %>/<%= @autofile %>/<%= @name %>.<%= @autofile %>
